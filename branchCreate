#!/bin/bash
echo hello world
x=1
y=1
j=99999999
while [ $x -le 10000 ]
do
  echo "Welcome $x times"
  string=Performance_Br_$((x))
  git branch $string
  git checkout $string
  touch Randomfile$((x))
  git push
  git push --set-upstream origin $string
while [ $y -le 200 ]
do
  string="touch randfile$((j+x)) && git add . && git commit -m '$((j+x)):randfile_$((j+x))'"
  y=$(( $y + 1 ))
done
  git push
  git push --set-upstream origin $string
  echo $string
  sleep 5.0
  x=$(( $x + 1 ))
done
