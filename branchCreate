  touch randomFile$x
  git add .
  git commit -m "s $x"
  #git push -u origin $string
  #git push -u origin $string
  git push --set-upstream origin $string
  git push -u origin $string
  echo $string
 #sleep 5.0
  x=$(( $x + 1 ))
done
