#!/bin/bash
echo hello world
x=1
y=1
j=99999999
while [ $x -le 10 ]
do
  echo "Welcome $x times"
  string=PPPerformance_Br_$((x))
  git branch $string main
  git checkout $string
  echo "Welcome $x times branch created" 
while [ $y -le 20 ]
do
  string="touch randfile$((j)) && git add . && git commit -m "sss $x $y"
  y=$(( $y + 1 ))
done
  git push -u origin $string
  git push --set-upstream origin $string
  echo $string
 #sleep 5.0
  x=$(( $x + 1 ))
done
