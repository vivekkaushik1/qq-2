#!/bin/bash
echo hello world
x=1
y=1
j=1
while [ $x -le 1 ]
do
  git fetch origin
  echo "Welcome $x times"
  string=PQP_BR_$((x))
  git branch $string
  git checkout $string
  touch randomFisle$x
  git add .
  git commit -m "s $x"
  #git push -u origin $string
  git push -u origin $string
  git push --set-upstream origin $string
  #git push -u origin $string
  echo $string

  x=$(( $x + 1 ))
done
